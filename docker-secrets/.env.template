# MapsBridge Environment Variables
# Copy this file to .env and fill in your values

# Google API configuration
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_API_ENABLED=false

# Security configuration
API_SECURITY_TOKEN=your_secure_token_here

# Spring profile
SPRING_PROFILES_ACTIVE=prod

# JVM options
JAVA_OPTS=-Xms256m -Xmx512m

# Map URL templates (optional - defaults will be used if not specified)
# MAPS_GOOGLE_URL=https://www.google.com/maps?q={lat},{lon}
# MAPS_APPLE_URL=https://maps.apple.com/?q={lat},{lon}
# MAPS_BING_URL=https://www.bing.com/maps?q={lat},{lon}
# MAPS_OSM_URL=https://www.openstreetmap.org/?mlat={lat}&mlon={lon}#map=16/{lat}/{lon}
# MAPS_WAZE_URL=https://waze.com/ul?ll={lat},{lon}&navigate=yes
# MAPS_KOMOOT_URL=https://www.komoot.com/plan/@{lat},{lon},9.000z?p[0]&p[1][loc]={lat},{lon}